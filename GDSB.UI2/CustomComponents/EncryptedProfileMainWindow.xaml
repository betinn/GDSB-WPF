<UserControl x:Class="GDSB.UI.CustomComponents.EncryptedProfileMainWindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GDSB.UI.CustomComponents"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="194"
             Cursor="Hand"
             Visibility="Visible"
             MinHeight="300"
             MinWidth="194"
             MaxWidth="194"
             MaxHeight="300"
             Margin="5,5,5,5">
    <Grid Cursor="Hand">
        <Image x:Name="img" Cursor="Hand" HorizontalAlignment="Left" Height="246" VerticalAlignment="Top" Width="191" OpacityMask="White"/>
        <Label x:Name="labelNome" Cursor="Hand" Content="Label" HorizontalAlignment="Center" Margin="0,247,0,0" VerticalAlignment="Top" Height="53" Width="194" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="14" Foreground="White" Background="{x:Null}"/>
    </Grid>
</UserControl>
